<h1>New Product</h1>

<%= simple_form_for @product, url: products_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :image %>
    <%= f.input :price %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />

<%= link_to 'Back', products_path %>
